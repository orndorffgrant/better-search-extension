{
  "manifest_version": 2,
  "name": "BetterSearch",
  "version": "0.1",
  "description": "Personalized search. Better than google.",
  "permissions": [
    "webRequest",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["indexer.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
